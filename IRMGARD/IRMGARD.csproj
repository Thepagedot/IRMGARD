    <Message Text="Creating OBBs..." />
    <XmlRead Prefix="n" Namespace="http://schemas.android.com/apk/res/android" XPath="/manifest/application/meta-data[@n:name='obbMainVersionCode']/@n:value" XmlFileName="Properties\AndroidManifest.xml">
      <Output TaskParameter="Value" PropertyName="OBBMainVersionCode" />
    </XmlRead>
    <XmlRead Prefix="n" Namespace="http://schemas.android.com/apk/res/android" XPath="/manifest/application/meta-data[@n:name='obbPatchVersionCode']/@n:value" XmlFileName="Properties\AndroidManifest.xml">
      <Output TaskParameter="Value" PropertyName="OBBPatchVersionCode" />
    </XmlRead>
    <XmlRead Prefix="n" Namespace="http://schemas.android.com/apk/res/android" XPath="/manifest/@package" XmlFileName="Properties\AndroidManifest.xml">
      <Output TaskParameter="Value" PropertyName="ManifestPackage" />
    </XmlRead>
    <Zip Files="@(MainAssetsFiles)" WorkingDirectory="$(ProjectDir)MainAssets" ZipFileName="main.$(OBBMainVersionCode).$(ManifestPackage).obb" ZipLevel="0" />
    <Zip Files="@(PatchAssetsFiles)" WorkingDirectory="$(ProjectDir)Assets" ZipFileName="patch.$(OBBPatchVersionCode).$(ManifestPackage).obb" ZipLevel="0" />
  </Target>
  <!-- END: Create OBBs -->
