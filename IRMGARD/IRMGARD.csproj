  <Choose>
    <When Condition="$(ZipAssets) == '1'">
      <PropertyGroup>
        <UpdateAndroidAssetsDependsOnTargets>$(CoreResolveReferencesDependsOn);_ComputeAndroidAssetsPaths;CreateAndroidAssetsDir;CreateOBBs</UpdateAndroidAssetsDependsOnTargets>
      </PropertyGroup>
    </When>
    <Otherwise>
      <PropertyGroup>
        <UpdateAndroidAssetsDependsOnTargets>$(CoreResolveReferencesDependsOn);_ComputeAndroidAssetsPaths;_GenerateAndroidAssetsDir</UpdateAndroidAssetsDependsOnTargets>
      </PropertyGroup>
    </Otherwise>
  </Choose>
  <!-- Override UpdateAndroidAssets from Xamarin.Android.Common.targets -->
  <Target Name="UpdateAndroidAssets" DependsOnTargets="$(UpdateAndroidAssetsDependsOnTargets)" />
  <Target Name="CreateAndroidAssetsDir">
    <MakeDir Directories="$(MonoAndroidAssetsDirIntermediate)" />
    <Copy SourceFiles="Assets\AboutAssets.txt" DestinationFiles="$(ProjectDir)$(MonoAndroidAssetsDirIntermediate)AboutAssets.txt" SkipUnchangedFiles="true" />
  </Target>
  <!-- Create OBBs -->
  <Target Name="CreateOBBs">
    <ItemGroup>
      <MainAssetsFiles Include="MainAssets\**\*.*" />
      <PatchAssetsFiles Include="Assets\**\*.*" Exclude="Assets\AboutAssets.txt;Assets\one_original.json;Assets\sandbox.json;Assets\test.json" />
